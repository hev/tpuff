{
  "name": "tpuff",
  "version": "0.1.0",
  "description": "A TypeScript CLI for interacting with turbopuffer",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "tpuff": "./dist/index.js",
    "tpuf": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "docker:build": "docker build -t tpuff-embeddings docker/",
    "docker:start": "docker run -d --name tpuff-embeddings -p 5050:5050 tpuff-embeddings",
    "docker:stop": "docker stop tpuff-embeddings && docker rm tpuff-embeddings",
    "docker:logs": "docker logs -f tpuff-embeddings",
    "docker:shell": "docker exec -it tpuff-embeddings /bin/bash"
  },
  "keywords": [
    "turbopuffer",
    "cli",
    "vector-database"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@turbopuffer/turbopuffer": "^1.0.0",
    "@xenova/transformers": "^2.17.0",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "commander": "^12.0.0",
    "tmp": "^0.2.5"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/tmp": "^0.2.6",
    "patch-package": "^8.0.1",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
